{
  "name": "backend",
  "version": "1.0.0",
  "description": "Tracker project",
  "main": "server.js",
  "scripts": {
    "make-seed": "npx knex seed:make dev --knexfile ./src/db/knexfile.js",
    "migrate:up": "node ./src/db/migrate.js migrate:latest",
    "migrate:down": "npx knex migrate:down --knexfile ./src/db/knexfile.js",
    "seed": "npx knex seed:run --knexfile ./src/db/knexfile.js",
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js"
  },
  "author": "Jeril",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.10.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3"
  }
}
